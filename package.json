  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "pg": "^6.4.2",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^3.1.0",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "assert": "^1.4.1",
